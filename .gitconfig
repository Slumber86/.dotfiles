[user]
	name = Agostino Marzotta
	email = agostino.marzotta@doctolib.com
[core]
	excludesfile = /Users/ago/.gitignore_global
[commit]
	template = /Users/ago/.stCommitMsg
	gpgsign = true
[rebase]
	autoStash = true
[pull]
	rebase = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[push]
	autoSetupRemote = true
